rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {

    match /users/{userId} {
      allow read, write: if request.auth.uid == userId;
    }

    match /public/{docId} {
      allow read: if true;
      allow write: if request.auth != null;
    }
  }
}
